<Application
    x:Class="BsodSimulator.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:BsodSimulator"
    xmlns:converters="using:BsodSimulator.Converters"
    RequestedTheme="Light">
    <Application.Resources>
        <converters:Int32ToDoubleConverter x:Key="IntDoubleTwoWayConverter"></converters:Int32ToDoubleConverter>
        <x:Double x:Key="NarrowMinWidth">0</x:Double>
        <x:Double x:Key="NormalMinWidth">521</x:Double>
        <x:Double x:Key="WideMinWidth">1200</x:Double>
        <converters:GenericObjectConverter x:Key="MakeCompilerHappy"></converters:GenericObjectConverter>
        <converters:InvertedBoolean x:Key="BooleanNotConverter"></converters:InvertedBoolean>

    </Application.Resources>
</Application>
